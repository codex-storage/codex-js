		".": "./dist/index.js"
	},
	"author": "Codex team",
	"readme": "README.md",
	"license": "MIT",
	"engines": {
		"node": ">=20"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@tsconfig/strictest": "^2.0.5",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"valibot": "^0.36.0"
	}
